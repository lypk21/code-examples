rn {\n        seller: {\n          id: qs.parse(location.search).id\n        }\n      }\n    },\n    computed: {\n      tabs() {\n        return [\n          {\n            label: '商品',\n            component: Goods,\n            data: {\n              seller: this.seller\n            }\n          },\n          {\n            label: '评论',\n            component: Ratings,\n            data: {\n              seller: this.seller\n            }\n          },\n          {\n            label: '商家',\n            component: Seller,\n            data: {\n              seller: this.seller\n            }\n          }\n        ]\n      }\n    },\n    created() {\n      this._getSeller()\n    },\n    methods: {\n      _getSeller() {\n        getSeller({\n          id: this.seller.id\n        }).then((seller) => {\n          this.seller = Object.assign({}, this.seller, seller)\n        })\n      }\n    },\n    components: {\n      Tab,\n      VHeader\n    }\n  }\n</script>\n\n<style lang=\"stylus\" scoped>\n  #app\n    .tab-wrapper\n      position: fixed\n      top: 136px\n      left: 0\n      right: 0\n      bottom: 0\n</style>\n"]}]}